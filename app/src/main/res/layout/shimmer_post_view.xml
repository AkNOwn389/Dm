<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <androidx.cardview.widget.CardView
        android:id="@+id/cardView36"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_margin="20dp"
        android:backgroundTint="@color/grey2"
        app:cardCornerRadius="25dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.cardview.widget.CardView
        android:id="@+id/cardView37"
        android:layout_width="0dp"
        android:layout_height="30dp"
        android:layout_marginStart="10dp"
        android:layout_marginEnd="50dp"
        android:backgroundTint="@color/grey2"
        app:cardCornerRadius="10dp"
        app:cardElevation="5dp"
        app:layout_constraintBottom_toBottomOf="@+id/cardView36"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/cardView36"
        app:layout_constraintTop_toTopOf="@+id/cardView36" />

    <androidx.cardview.widget.CardView
        android:id="@+id/cardView38"
        android:layout_width="80dp"
        android:layout_height="10dp"
        android:layout_marginTop="10dp"
        android:backgroundTint="@color/grey2"
        app:cardCornerRadius="5dp"
        app:layout_constraintStart_toStartOf="@+id/cardView37"
        app:layout_constraintTop_toBottomOf="@+id/cardView37" />

    <androidx.cardview.widget.CardView
        android:id="@+id/cardView39"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_margin="20dp"
        android:backgroundTint="@color/grey2"
        app:cardCornerRadius="10dp"
        app:cardElevation="5dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cardView38" />

    <androidx.cardview.widget.CardView
        android:id="@+id/cardView40"
        android:layout_width="match_parent"
        android:layout_height="300dp"
        android:layout_margin="20dp"
        android:backgroundTint="@color/grey2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.4"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cardView39" />

    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:backgroundTint="@color/grey2"
        app:cardCornerRadius="10dp"
        android:layout_marginTop="40dp"
        android:layout_marginEnd="30dp"
        android:layout_marginStart="30dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cardView40" />

    <androidx.cardview.widget.CardView
        android:id="@+id/cardView41"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:layout_marginTop="10dp"
        android:backgroundTint="@color/grey2"
        app:cardCornerRadius="10dp"
        app:layout_constraintStart_toStartOf="@+id/cardView40"
        app:layout_constraintTop_toBottomOf="@+id/cardView40" />

    <androidx.cardview.widget.CardView
        android:id="@+id/cardView42"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="10dp"
        android:backgroundTint="@color/grey2"
        app:cardCornerRadius="10dp"
        app:layout_constraintStart_toEndOf="@+id/cardView41"
        app:layout_constraintTop_toBottomOf="@+id/cardView40" />

    <androidx.cardview.widget.CardView
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:layout_marginTop="10dp"
        android:backgroundTint="@color/grey2"
        app:cardCornerRadius="10dp"
        android:layout_marginStart="10dp"
        app:layout_constraintStart_toEndOf="@+id/cardView42"
        app:layout_constraintTop_toBottomOf="@+id/cardView40" />
</androidx.constraintlayout.widget.ConstraintLayout>